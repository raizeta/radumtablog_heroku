{% extends "BackBundle::layout.html.twig" %}

{% block title %}BackBundle:List:index{% endblock %}

{% block body %}
<div class="content-wrapper">


        <!-- Main content -->
        <section class="content">
          <div class="row">
            <div class="col-md-12">
              <div class="box">
                <div class="box-header">
                  <h3 class="box-title">List Of User</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                  <table class="table table-bordered">
                    <tr>
                      <th style="width: 10px">ID</th>
                      <th>Username</th>
                      <th>Email</th>
                      <th>Last Login</th>
                      <th>Nama</th>
                    </tr>
                    {% for entities in entities %}
                    <tr>
                      <td>{{entities.id}}</td>
                      <td>{{entities.username}}</td>
                      <td>{{entities.email}}</td>
                      <td>{{entities.lastLogin|date('D, d/m/Y')}}</td>
                      <td>{{entities.namauser}}</td>
                    </tr>
                    {% endfor %}
                  </table>
                </div><!-- /.box-body -->
                <div class="box-footer clearfix">
                  {{ knp_pagination_render(entities) }}
                </div>
              </div><!-- /.box -->
            </div><!-- /.col -->
          </div><!-- /.row -->

        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->
{% endblock %}
